      </eAnnotations>
      <eParameters name="element">
        <eGenericType eTypeParameter="#//DisplayOption/T"/>
      </eParameters>
    </eOperations>
    <eOperations name="getDefaultProperties">
      <eGenericType eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EMap">
