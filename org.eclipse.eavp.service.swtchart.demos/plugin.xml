<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="org.eclipse.eavp.service.swtchart.demos.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesPreferencePage"
            id="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesPreferencePage"
            name="Line Series Chart Settings">
      </page>
      <page
            category="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesPreferencePage"
            class="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesPrimaryAxesPreferencePage"
            id="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesPrimaryAxesPreferencePage"
            name="Primary Axes">
      </page>    
      <page
            category="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesPreferencePage"
            class="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesSecondaryAxesPreferencePage"
            id="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesSecondaryAxesPreferencePage"
            name="Secondary Axes">
      </page>
      <page
            category="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesPreferencePage"
            class="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesDataPreferencePage"
            id="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesDataPreferencePage"
            name="Data">
      </page>                  
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.eclipse.eavp.service.swtchart.demos.preferences.LineSeriesPreferenceInitializer">
      </initializer>
   </extension>

</plugin>
